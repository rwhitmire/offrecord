{
  "name": "offrecord",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run dev:server & npm run dev:client",
    "dev:server": "set DEBUG=offrecord:* && nodemon ./bin/www --ignore client --ignore public",
    "dev:client": "set NODE_ENV=development& webpack -w",
    "start": "node ./bin/www",
    "deploy": "git push heroku master",
    "postinstall": "webpack"
  },
  "dependencies": {
    "atob": "^2.0.3",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "~1.13.2",
    "btoa": "^1.1.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "emojione": "^2.2.0",
    "express": "~4.13.1",
    "hbs": "~3.1.0",
    "linkify-it": "^1.2.4",
    "morgan": "~1.6.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "serve-favicon": "~2.3.0",
    "shortid": "^2.2.6",
    "socket.io": "^1.4.6",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "nodemon": "^1.9.2"
  },
  "engines": {
    "node": "4.4.5"
  }
